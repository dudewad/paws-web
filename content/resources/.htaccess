<IfModule mod_headers.c>
    Header always append Access-Control-Allow-Origin: "http://pawsforabeer.com"
    Header always append Access-Control-Allow-Origin: "http://www.pawsforabeer.com"
    Header always append Access-Control-Allow-Origin: "http://asset.pawsforabeer.com"
    Header always append Access-Control-Allow-Origin: "http://staging.pawsforabeer.com"
    Header always append Access-Control-Allow-Origin: "http://s-asset.pawsforabeer.com"
    Header add Access-Control-Allow-Headers "origin, x-requested-with, content-type"
    Header add Access-Control-Allow-Methods "PUT, GET, POST, DELETE, OPTIONS"
</IfModule>
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule . index.html [L]